{
  "name": "@clix-so/clix-cli",
  "version": "1.0.2",
  "description": "A CLI tool for integrating and managing the Clix SDK in your mobile projects",
  "type": "module",
  "bin": {
    "clix": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && node scripts/add-shebang.js",
    "build:only": "tsup",
    "postbuild": "node scripts/add-shebang.js",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "clix",
    "sdk",
    "cli",
    "mobile",
    "ios",
    "android",
    "push-notifications"
  ],
  "author": "Clix",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clix-so/homebrew-clix-cli.git"
  },
  "homepage": "https://github.com/clix-so/homebrew-clix-cli",
  "dependencies": {
    "chalk": "^5.3.0",
    "execa": "^9.5.2",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "meow": "^13.2.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
